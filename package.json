{
  "name": "track-moto-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "5.0.6",
    "react-scripts": "1.0.17",
    "redux": "3.7.2",
    "redux-form": "^7.1.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "eslint": "4.10.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "jshint": "^2.9.5",
    "node-sass": "^4.6.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "4.1.2",
    "react-scripts": "1.0.17",
    "redux-devtools": "3.4.0"
  },
  "scripts": {
    "build-css2": "node-sass src/ -o src/",
    "watch-css2": "npm run build-css && node-sass src/ -o src/ --watch --recursive",

    "build-css": "node-sass --include-path scss src/ -o src/",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",

    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all -s build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
